{
	"name": "nodecg-for-smash",
	"version": "0.3.0",
	"description": "NodeCG bundle for SSBM streams",
	"hompeage": "https://github.com/mparkms/nodecg-for-smash",
	"license": "MIT",
	"author": "Mike Park <mikeparkms@gmail.com> (www.mikeparkms.com)",
	"dependencies": {
		"obs-remote": "^1.0.1"
	},
	"nodecg": {
		"compatibleRange": "~0.8.0",
		"dashboardPanels": [
			{
				"name": "ssbm-players",
				"title": "Players & Score",
				"width": 4,
				"headerColor": "#2d4e8a",
				"file": "players.html"
			},
			{
				"name": "ssbm-bracket",
				"title": "Bracket",
				"width": 2,
				"headerColor": "#2d4e8a",
				"file": "bracket.html"
			},
			{
				"name": "ssbm-misc-text",
				"title": "Misc Text",
				"width": 2,
				"headerColor": "#2d4e8a",
				"file": "misc-text.html"
			},
			{
				"name": "manual-update",
				"title": "Manual Update",
				"width": 2,
				"dialog": true,
				"dialogButtons": [
					{
						"name": "update",
						"type": "confirm"
					},
					{
						"name": "done",
						"type": "dismiss"
					}
				],
				"file": "manual-update.html"
			},
			{
				"name": "manage-teams",
				"title": "Manage Team Icons",
				"width": 3,
				"dialog": true,
				"dialogButtons": [
					{
						"name": "update",
						"type": "confirm"
					},
					{
						"name": "done",
						"type": "dismiss"
					}
				],
				"file": "manage-teams.html"
			}
		],
		"graphics": [
			{
				"file": "players.html",
				"width": 1290,
				"height": 1060
			},
			{
				"file": "bracket.html",
				"width": 1500,
				"height": 900
			},
			{
				"file": "crew-roster.html",
				"width": 550,
				"height": 330
			},
			{
				"file": "playercam.html",
				"width": 500,
				"height": 40
			},
			{
				"file": "lower-third.html",
				"width": 1700,
				"height": 250
			},
			{
				"file": "top-info.html",
				"width": 1290,
				"height": 60
			}
		],
		"assetCategories": [
			{
				"name": "teamlogos",
				"title": "Team Logos",
				"allowedTypes": [
					"jpg",
					"jpeg",
					"gif",
					"png"
				]
			}
		]
	}
}